<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="my.edu.tarc.secondhandcar">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity" />

        <activity
            android:name=".ResetPwActivity"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".RegistrationActivity"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".ViewProfileActivity"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".EditProfActivity"
            android:parentActivityName=".ViewProfileActivity" />
        <activity
            android:name=".ChangePasswordActivity"
            android:parentActivityName=".ViewProfileActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".searchCarActivity" />
        <activity
            android:name=".AdvSearchActivity"
            android:parentActivityName=".searchCarActivity">

        </activity>
        <activity
            android:name=".CarActivity"
            android:parentActivityName=".SearchCarResultActivity"></activity>
        <activity
            android:name=".SearchCarResultActivity"
            android:parentActivityName=".searchCarActivity"></activity>
        <activity
            android:name=".MakeAppointmentActivity"
            android:parentActivityName=".CarActivity"></activity>
        <activity
            android:name=".BookingDetailActivity"
            android:parentActivityName=".MainActivity"></activity>

        <receiver android:name=".broadcastReceiver">
            <intent-filter>

            </intent-filter>

        </receiver>
    </application>


</manifest>